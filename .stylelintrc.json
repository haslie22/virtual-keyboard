{
  "extends": [
    "stylelint-config-standard-scss"
  ],
  "rules": {
    "at-rule-empty-line-before": ["always", {"except": ["after-same-name"]}],
    "selector-class-pattern": ["[a-z]+((__[a-z]+)?(_[a-z]+)?)?", {"resolveNestedSelectors": true }],
    "scss/dollar-variable-empty-line-before": null
  },
  "fix": true
}
